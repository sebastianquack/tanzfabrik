<% url = edit_admin_content_module_url(cm.id) if cm.id? %>
<li>
  <div>
    headline: <b><%= cm.headline %></b><br>
    module_type: <%= cm.module_type %><br>
    style_option: <%= cm.style_option %><br>
    <% if cm.images.length > 0 %>
      images:
      <% cm.images.each do |image| %>
        <%= image_tag image.attachment.url(:thumb) %>
      <% end %>
      <br>
    <% end %>

    <% if cm.downloads.length > 0 %>
      downloads: 
      <% cm.downloads.each do |dl| %>
        <%= link_to dl.description_de, dl.attachment.url %>
      <% end %>
      <br>
    <% end %>
  </div>
  <br>
  <%= link_to "edit", url, :class => "button" %>
</li>